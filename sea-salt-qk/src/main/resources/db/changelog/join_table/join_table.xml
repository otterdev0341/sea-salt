    <changeSet id="14" author="otterdev_ball">
        <comment>relation of sale x file to sale</comment>
        <addForeignKeyConstraint
            baseTableName="sale_file"
            baseColumnNames="sale_id"
            constraintName="fk_sale_x_file_sale"
